\r\n    },\r\n    methods: {\r\n            async submitForm(){\r\n                await axios.post('api/loginUser',{\r\n                    email: this.email,\r\n                    password: this.password,\r\n                })\r\n                .then(function(response){\r\n                    if(response.status==200){\r\n                    console.log(response);\r\n                    this.token=response.data.token;\r\n                    this.response=response.data.description;\r\n                    this.username=response.data.user;\r\n                    let d = new Date();\r\n                    d.setTime(d.getTime() + 1 * 24 * 60 * 60 * 1000);\r\n                    let expires = \"expires=\" + d.toUTCString();\r\n                    //document.cookie = \"Token=\" + response.data.token + \";\" + expires + \";path=/\";\r\n                    this.$cookies.set(\"token\",\"Token=\" + response.data.token + \";\" + expires + \";path=/\")\r\n                    }\r\n                })\r\n                .catch(error =>{\r\n                    console.log(error);\r\n                })\r\n                \r\n            },\r\n            isAuthenticated(){\r\n                if(this.token)return true;\r\n                else return false;\r\n            }\r\n        }\r\n\r\n}\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}