i=0;i<res.length;i++){\r\n            let category=res[i].category.join()\r\n            res[i]['category']=category\r\n            let keywords= res[i].keywords.join()\r\n            res[i]['keywords']= keywords\r\n            console.log(res[i])\r\n        }\r\n        this.resTable = this.resTable.concat(res);\r\n      },\r\n      setCurrentPage(page) {\r\n        this.currentPage = page;\r\n      },\r\n    },\r\n  };\r\n  </script>\r\n  "]},"metadata":{},"sourceType":"module","externalDependencies":[]}