{"ast":null,"code":"import axios from 'axios';\nimport NetworkGraph from '../components/NetworkGraphView.vue';\nexport default {\n  name: 'searchWord',\n  components: {\n    NetworkGraph\n  },\n  data() {\n    return {\n      response: '',\n      category: false,\n      keyword: false,\n      searchword: '',\n      resTable: [],\n      nodeList: {},\n      edgeList: {}\n    };\n  },\n  methods: {\n    async submitForm() {\n      var res = [];\n      this.nodeList = {};\n      this.edgeList = {};\n      this.resTable = [];\n      await axios.post('api/searchGraphKeyword', {\n        searchword: this.searchword,\n        category: this.category,\n        keyword: this.keyword\n      }).then(function (response) {\n        if (response.status == 200) {\n          res = response.data; //copy list of records from backend to global variable\n          res.forEach((item, index) => {\n            item['category'] = item.category.join();\n            item['keywords'] = item.keywords.join();\n          });\n        }\n      }).catch(error => {\n        console.log(error);\n      });\n      // Reset form fields\n      this.searchword = '';\n      this.category = false;\n      this.keyword = false;\n      this.$refs.form.reset();\n      // clear the input values for the form data fields\n      this.resTable = this.resTable.concat(res);\n      const nodes = {};\n      const edges = {};\n      this.resTable.forEach((item, index) => {\n        // Create a new node if it doesn't already exist\n        if (!nodes[item.imagename]) {\n          nodes[item.imagename] = {\n            name: item.imagename,\n            type: \"image\",\n            color: \"#ff0000\"\n          };\n        }\n        if (!nodes[item.searchword]) {\n          nodes[item.searchword] = {\n            name: item.searchword,\n            type: \"linkword\",\n            color: \"#4466cc\"\n          };\n        }\n\n        // Create a new edge if it doesn't already exist\n        const source = item.searchword;\n        const target = item.imagename;\n        const edgeId = `edge${index + 1}`;\n        if (!edges[edgeId]) {\n          edges[edgeId] = {\n            source,\n            target\n          };\n        }\n      });\n\n      // Update state with new node and edge lists\n      this.nodeList = nodes;\n      this.edgeList = edges;\n    }\n  }\n};","map":{"version":3,"mappings":"AAwCA,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,YAAW,MAAO,oCAAmC;AAE5D,eAAc;EACVC,IAAI,EAAE,YAAY;EAClBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAI,GAAG;IACH,OAAM;MACFC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,KAAK;MACfC,OAAO,EAAE,KAAK;MACdC,UAAU,EAAE,EAAE;MACdC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,CAAC,CAAC;MACZC,QAAQ,EAAE,CAAC;IACf;EACJ,CAAC;EACDC,OAAO,EAAE;IACD,MAAMC,UAAU,GAAE;MACd,IAAIC,GAAG,GAAC,EAAC;MACT,IAAI,CAACJ,QAAQ,GAAC,CAAC;MACf,IAAI,CAACC,QAAQ,GAAC,CAAC;MACf,IAAI,CAACF,QAAQ,GAAC,EAAC;MACf,MAAMT,KAAK,CAACe,IAAI,CAAC,wBAAwB,EAAC;QACtCP,UAAU,EAAE,IAAI,CAACA,UAAU;QAC3BF,QAAQ,EAAC,IAAI,CAACA,QAAQ;QACtBC,OAAO,EAAE,IAAI,CAACA;MAClB,CAAC,EACAS,IAAI,CAAC,UAASX,QAAQ,EAAC;QACpB,IAAGA,QAAQ,CAACY,MAAM,IAAE,GAAG,EAAC;UACxBH,GAAG,GAACT,QAAQ,CAACD,IAAI,EAAE;UACnBU,GAAG,CAACI,OAAO,CAAC,CAACC,IAAI,EAACC,KAAK,KAAG;YACtBD,IAAI,CAAC,UAAU,CAAC,GAACA,IAAI,CAACb,QAAQ,CAACe,IAAI,EAAC;YACpCF,IAAI,CAAC,UAAU,CAAC,GAACA,IAAI,CAACG,QAAQ,CAACD,IAAI,EAAC;UACxC,CAAC;QACD;MACJ,CAAC,EACAE,KAAK,CAACC,KAAI,IAAI;QACXC,OAAO,CAACC,GAAG,CAACF,KAAK,CAAC;MACtB,CAAC;MACD;MACA,IAAI,CAAChB,UAAS,GAAI,EAAE;MACpB,IAAI,CAACF,QAAO,GAAI,KAAK;MACrB,IAAI,CAACC,OAAM,GAAI,KAAK;MACpB,IAAI,CAACoB,KAAK,CAACC,IAAI,CAACC,KAAK,EAAE;MACvB;MACA,IAAI,CAACpB,QAAQ,GAAC,IAAI,CAACA,QAAQ,CAACqB,MAAM,CAAChB,GAAG;MAEtC,MAAMiB,KAAI,GAAI,CAAC,CAAC;MAChB,MAAMC,KAAI,GAAI,CAAC,CAAC;MAEhB,IAAI,CAACvB,QAAQ,CAACS,OAAO,CAAC,CAACC,IAAI,EAAEC,KAAK,KAAK;QACvC;QACA,IAAI,CAACW,KAAK,CAACZ,IAAI,CAACc,SAAS,CAAC,EAAE;UACxBF,KAAK,CAACZ,IAAI,CAACc,SAAS,IAAI;YAAE/B,IAAI,EAAEiB,IAAI,CAACc,SAAS;YAAEC,IAAI,EAAE,OAAO;YAAEC,KAAK,EAAC;UAAU,CAAC;QACpF;QAEA,IAAI,CAACJ,KAAK,CAACZ,IAAI,CAACX,UAAU,CAAC,EAAE